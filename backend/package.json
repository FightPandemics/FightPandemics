    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "env-schema": "^1.1.0",
    "fastify": "^2.14.1",
    "fastify-authz-jwks": "^1.0.11",
    "fastify-jwt": "^1.3.1",
    "fastify-oas": "^2.7.0",
    "fastify-plugin": "^1.6.1",
    "fastify-sensible": "^2.1.1",
    "fluent-schema": "^0.10.0",
    "http-errors": "^1.7.3",
    "moment": "2.25.3",
