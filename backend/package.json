{
  "name": "fightpandemics",
  "version": "1.0.0",
  "private": true,
  "description": "Help to to fight global pandemics",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --inspect=0.0.0.0:9229 -L server.js",
