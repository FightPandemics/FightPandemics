{
  "name": "fightpandemics",
  "version": "1.0.0",
  "private": true,
  "description": "Help to to fight global pandemics",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon -L server.js",
    "test": "jest",
    "lint": "eslint . --fix",
    "schemas": "node ./generateSchemas.js",
    "install-docker": "docker-compose run backend-service npm install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "keywords": [
    "fight",
    "pandemics"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "fastify": "^2.13.0",
    "fastify-authz-jwks": "^1.0.11",
    "fastify-jwt": "^1.3.1",
    "fastify-plugin": "^1.6.1",
    "http-errors": "^1.7.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.6",
    "node-cache": "^5.1.0",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "@openapi-contrib/openapi-schema-to-json-schema": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-utils": "^2.0.0",
    "husky": "^4.2.3",
    "js-yaml": "^3.13.1",
    "lint-staged": "^10.1.1",
    "nock": "^12.0.3",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.2",
    "supertest": "^4.0.2"
  }
}
