    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-utils": "^2.0.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.1",
    "nock": "^12.0.3",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.2",
    "supertest": "^4.0.2"
